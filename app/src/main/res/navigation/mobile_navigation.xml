<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="dev.syorito_hatsuki.onlyone.ui.users.MainFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/goToUserPage"
            app:destination="@id/blankFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="dev.syorito_hatsuki.onlyone.ui.MyProfile.MyProfile"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="dev.syorito_hatsuki.onlyone.ui.users.MainFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="dev.syorito_hatsuki.onlyone.ui.fragments.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <argument
            android:name="UserID"
            app:argType="integer" />
    </fragment>
</navigation>